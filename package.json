  },
  "scripts": {
    "test": "NODE_ENV=test YOURPACKAGE_COVERAGE=1 ./node_modules/mocha/bin/mocha --reporter mocha-lcov-reporter | ./node_modules/coveralls/bin/coveralls.js",
